<% if false %>
<%- model_class = StudySession -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>



<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:title) %></th>
      <th><%= model_class.human_attribute_name(:room_number) %></th>
      <th><%= model_class.human_attribute_name(:date) %></th>
      <th><%= model_class.human_attribute_name(:time) %></th>
      <th><%= model_class.human_attribute_name(:tenant_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @study_sessions.each do |study_session| %>
      <tr>
        <td><%= link_to study_session.id, study_session_path(study_session) %></td>
        <td><%= study_session.title %></td>
        <td><%= study_session.room_number %></td>
        <td><%= study_session.date %></td>
        <td><%= study_session.time %></td>
        <td><%= study_session.tenant_id %></td>
        <td><%=l study_session.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_study_session_path(study_session), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      study_session_path(study_session),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_study_session_path,
            :class => 'btn btn-primary' %>
            
<% end %>           
            
            
  
  
<%- model_class = StudySession -%>

<div class="page-header">

<h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>

</div>

<% if @study_sessions.size > 0 %>

<table class="table table-striped">

<thead>

<tr>

<th><%= model_class.human_attribute_name(:title) %></th>

<th><%= model_class.human_attribute_name(:room_number) %></th>

<th><%= model_class.human_attribute_name(:date) %></th>

<th><%= model_class.human_attribute_name(:time) %></th>

<th><%=t '.actions', :default => t("helpers.actions") %></th>

</tr>

</thead>

<tbody>

<% @study_sessions.each do |study_session| %>

<tr>

<td><%= link_to study_session.title,

tenant_study_session_path(study_session, tenant_id: @tenant.id), class: 'main-link' %></td>

<td><%= study_session.room_number %></td>

<td><%= study_session.date %></td>

<td><%= study_session.time %></td>

<td>

<%= link_to t('.edit', :default => t("helpers.links.edit")),

edit_tenant_study_session_path(study_session, tenant_id: @tenant.id), :class => 'btn btn-default btn-xs' %>

<%= link_to 'Edit Users', tenant_study_session_path(study_session, tenant_id: @tenant.id),

:class => 'btn btn-default btn-xs' %>

<%= link_to t('.destroy', :default => t("helpers.links.destroy")),

tenant_study_session_path(study_session, tenant_id: @tenant.id),

:method => :delete,

:data => { :confirm => t('.confirm', :default => t("helpers.links.confirm",

:default => 'Are you sure?')) },

:class => 'btn btn-xs btn-danger' %>

</td>

</tr>

<% end %>

</tbody>

</table>

<% else %>

<p class="lead">You don't have any study_sessions</p>

<% end %>



<% if (@tenant.can_create_study_sessions?) %>

<%= link_to 'New Study Session',

new_tenant_study_session_path(tenant_id: @tenant.id),

:class => 'btn btn-primary' %>

<% end %>