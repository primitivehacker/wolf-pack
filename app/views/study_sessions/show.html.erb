<%- model_class = StudySession -%>
<div class="page-header">

<h3><%= @study_session.title %></h3>

<h3><small>Organization - <%= tenant_name @study_session.tenant_id %></small></h3>

</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:title) %>:</strong></dt>
  <dd><%= @study_session.title %></dd>
  <dt><strong><%= model_class.human_attribute_name(:room_number) %>:</strong></dt>
  <dd><%= @study_session.room_number %></dd>
  <dt><strong><%= model_class.human_attribute_name(:date) %>:</strong></dt>
  <dd><%= @study_session.date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:time) %>:</strong></dt>
  <dd><%= @study_session.time %></dd>
  <dt><strong><%= model_class.human_attribute_name(:tenant_id) %>:</strong></dt>
  <dd><%= @study_session.tenant_id %></dd>
</dl>

<%= link_to '< Back to Organization Home', root_path, :class => 'btn btn-default' %>

<%= link_to t('.edit', :default => t("helpers.links.edit")),

edit_tenant_study_session_path(study_session: @study_session, tenant: @study_session.tenant_id), :class => 'btn btn-default' %>

<%= link_to t('.destroy', :default => t("helpers.links.destroy")),

tenant_study_session_path(@study_session, @study_session.tenant_id),

:method => 'delete',

:data => { :confirm => t('.confirm', :default => t("helpers.links.confirm",

:default => 'Are you sure?')) },

:class => 'btn btn-danger' %>


